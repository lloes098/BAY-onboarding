#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jian/Desktop/wk4/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/plugins/node_modules:/Users/jian/Desktop/wk4/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/node_modules:/Users/jian/Desktop/wk4/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2__typescript@5.9.2_/node_modules:/Users/jian/Desktop/wk4/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jian/Desktop/wk4/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/plugins/node_modules:/Users/jian/Desktop/wk4/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/node_modules:/Users/jian/Desktop/wk4/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2__typescript@5.9.2_/node_modules:/Users/jian/Desktop/wk4/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../plugins/bin.js" "$@"
else
  exec node  "$basedir/../../plugins/bin.js" "$@"
fi
